            <string>/// this is camera code

if instance_exists(objMyChar) and !global.gamePaused {
    
    if wait = 0 then wait = 1; // prevent zero division?
    
    if xxprev != xx then xx = lerp(xxprev,xx,16);
    if yyprev != yy then yy = lerp(yyprev,yy,16);
    
    var max_x = room_width - view_wview[0];
    var max_y = room_height - view_hview[0];
    
    xx += ((global.targetX - view_wview[0] / 2) - xx) / wait;
    if xx &lt; 0 then xx = 0;
    if xx &gt; max_x then xx = max_x;
    
    yy += ((global.targetY - view_hview[0] / 2) - yy) / wait;
    if yy &lt; 0 then yy = 0;
    if yy &gt; max_y then yy = max_y;
    
    view_xview[0] = round(xx);
    view_yview[0] = round(yy);
    
    xxprev = xx;
    yyprev = yy;
}
</string>
